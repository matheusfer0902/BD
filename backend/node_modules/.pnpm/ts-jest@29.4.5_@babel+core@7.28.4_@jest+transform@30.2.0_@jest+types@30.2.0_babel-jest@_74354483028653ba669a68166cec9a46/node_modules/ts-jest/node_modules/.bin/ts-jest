else
  export NODE_PATH="/Users/matheusferreira/Documents/ufpb/bd/BD/backend/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@7.28.4_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_74354483028653ba669a68166cec9a46/node_modules/ts-jest/node_modules:/Users/matheusferreira/Documents/ufpb/bd/BD/backend/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@7.28.4_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_74354483028653ba669a68166cec9a46/node_modules:/Users/matheusferreira/Documents/ufpb/bd/BD/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
