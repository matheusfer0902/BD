#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/matheusferreira/Documents/ufpb/bd/BD/backend/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@7.28.4_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_bdd021bfe4ee9f785cd610f3721034aa/node_modules/ts-jest/node_modules:/Users/matheusferreira/Documents/ufpb/bd/BD/backend/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@7.28.4_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_bdd021bfe4ee9f785cd610f3721034aa/node_modules:/Users/matheusferreira/Documents/ufpb/bd/BD/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/matheusferreira/Documents/ufpb/bd/BD/backend/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@7.28.4_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_bdd021bfe4ee9f785cd610f3721034aa/node_modules/ts-jest/node_modules:/Users/matheusferreira/Documents/ufpb/bd/BD/backend/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@7.28.4_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_bdd021bfe4ee9f785cd610f3721034aa/node_modules:/Users/matheusferreira/Documents/ufpb/bd/BD/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ts-jest@29.4.5_@babel+core@7.28.4_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_bdd021bfe4ee9f785cd610f3721034aa/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/ts-jest@29.4.5_@babel+core@7.28.4_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_bdd021bfe4ee9f785cd610f3721034aa/node_modules/ts-jest/cli.js" "$@"
fi
